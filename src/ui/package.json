{
  "name": "ezratask-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
        "scripts": {
          "dev": "vite",
          "build": "run-p type-check \"build-only {@}\" --",
          "preview": "vite preview",
          "test:unit": "echo \"ERROR: Local tests are not reliable. Please run unit tests via Docker. See README.md for instructions.\" && exit 1",
          "test:e2e": "echo \"ERROR: Local E2E tests are not reliable. Please run E2E tests via Docker. See README.md for instructions.\" && exit 1",
          "_test:unit:local": "vitest run",
          "_test:e2e:local": "cypress run",
          "test:unit:docker": "docker-compose run --rm ui npm run _test:unit:local",
          "test:e2e:docker": "docker-compose run --rm e2e",
          "test:e2e:open": "cypress open",
          "build-only": "vite build",
          "type-check": "vue-tsc --build",    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^9.1.15",
    "@storybook/addon-docs": "^9.1.15",
    "@storybook/addon-onboarding": "^9.1.15",
    "@storybook/vue3-vite": "^9.1.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.23",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cross-env": "^10.1.0",
    "cypress": "^15.5.0",
    "eslint": "^9.37.0",
    "eslint-plugin-storybook": "^9.1.15",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "storybook": "^9.1.15",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}
